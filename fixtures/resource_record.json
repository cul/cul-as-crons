		{
			"jsonmodel_type": "note_multipart",
			"persistent_id": "102a5133cf182a981dca58b25eeabd82",
			"label": "Restrictions on Access",
			"type": "accessrestrict",
			"rights_restriction": {
				"local_access_restriction_type": [

				]
			},
			"subnotes": [
				{
					"jsonmodel_type": "note_text",
					"content": "The following boxes are located off-site: Box 1. You will need to request this material from the Rare Book and Manuscript Library at least three business days in advance to use the collection in the Rare Book and Manuscript Library reading room. Materials in map case 14-P-1 are located on-site.",
					"publish": true
				}
			],
			"publish": true
		},
		{
			"jsonmodel_type": "note_multipart",
			"persistent_id": "31f474efb506e25c730bb00fc487940b",
			"label": "Summary",
			"type": "scopecontent",
			"subnotes": [
				{
					"jsonmodel_type": "note_text",
					"content": "This collection of printed materials and photographs was assembled by Mary Alden Hopkins, a member of the Expedition, in 1952. The content primarily spans the time period just before, during, and after the expedition between the winter of 1915 and 1916. The collection centers on Hopkins' personal accounts of the expedition, biographical pamphlets, and passenger lists. Official statements issued by Louis P. Lochner, General Secretary of The Neutral Conference for Continuous Mediation, at Stockholm, are also included herein. Additionally, a letter from Lochner with the \"Appeal to Neutrals\" the Neutral Conference's manifesto, is located in this collection. Several issues of Four Lights, a magazine published by the Women's Peace Party of New York City, are included along with various newspaper clippings pertaining both to the expedition and to the war in general. The photographs in this collection are both unidentified and undated, but certainly describe the social and political activities that took place during the expedition.",
					"publish": true
				}
			],
			"publish": true
		},
		{
			"jsonmodel_type": "note_multipart",
			"persistent_id": "94bb8b75924a1b69afb4a3078fa476e9",
			"label": "Preferred Citation",
			"type": "prefercite",
			"subnotes": [
				{
					"jsonmodel_type": "note_text",
					"content": "Identification of specific item; Date (if known); Henry Ford Peace Expedition collection; Box and Folder; Rare Book and Manuscript Library, Columbia University Library.",
					"publish": true
				}
			],
			"publish": true
		},
